                                <field name="ks_df_report_account_type"/>
                            </group>
                        </group>
                        <notebook invisible="ks_df_report_account_type not in ['accounts','ks_coa_type', 'account_report']">
                            <page name="dynamic_report_lines" string="Dynamic Report">
                                <group>
                                    <field name="ks_df_report_account_report_ids" invisible="ks_df_report_account_type != 'account_report'"
