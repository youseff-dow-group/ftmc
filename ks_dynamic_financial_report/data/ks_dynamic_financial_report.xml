                                <field name="ks_df_report_account_ids" invisible="ks_df_report_account_type != 'accounts'"/>
                                <field name="ks_dfr_account_type_ids" invisible="ks_df_report_account_type != 'ks_coa_type'"/>
                            </page>
                        </notebook>
                        <notebook invisible="ks_parent_id != False" >
                            <page name="dynamic_report_filters" string="Dynamic Report Filters">
                                <group>
                                    <group>
